   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0.9999999999999999\n",
      "1.0\n"
     ]
    }
   ],
   "source": [
    "# Define your matrix as a NumPy array\n",
    "U = np.array([[1/math.sqrt(2), 1/math.sqrt(2)],\n",
    "\t\t\t\t   [1/math.sqrt(2), -1/math.sqrt(2)]])\n",
    "\n",
    "# Define ∣ψ⟩\n",
    "psi = np.array([[(1+2j)/3],\n",
    "\t\t\t   [-2/3]])\n",
    "\n",
    "print(np.linalg.norm(np.dot(U, psi)))\n",
    "print(np.linalg.norm(psi))"
   ]
